{
    "manifest_version": 2,
    "name": "Elysian",
    "description": "A browser extension to backup regularly used bookmarks of your browser to your home lab.",
    "version": "0.9.0",
    "author": "aadityajoshi151@gmail.com",
    "permissions": ["bookmarks","notifications","webRequest","tabs","storage"],
    "icons": {
        "16": "assets/icons/icon16.png",
        "32": "assets/icons/icon32.png",
        "48": "assets/icons/icon48.png",
        "128": "assets/icons/icon128.png"
      },    
    "browser_action": {
        "default_popup": "src/Foreground/Popup/popup.html"
    },
    "background": {
        "scripts": ["src/Background/background.js"],
        "persistent": false,
        "type": "module"
      }
}