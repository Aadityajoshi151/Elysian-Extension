{
    "manifest_version": 3,
    "name": "Elysian",
    "description": "A self-hosted tool to backup your regularly used bookmarks from the bookmarks toolbar of your browser to your home lab.",
    "version": "0.9.0",
    "permissions": ["bookmarks","notifications","webRequest","tabs","storage"],
    "action": {
        "default_popup": "src/Foreground/Popup/popup.html"
    },
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "service_worker": "src/Background/background.js"
      }
}